{
  "name": "hrnet-plugin",
  "private": false,
  "version": "1.1.0",
  "type": "module",
  "license": "MIT",
  "description": "A simple plugin hrnet date picker",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint --fix './lib/**/*.{ts,tsx,js,jsx}' && prettier . --write './lib/**/*.{ts,tsx,js,jsx,css}'",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:openclassrooms-students/hrnet-plugin.git"
  },
  "keywords": [
    "react",
    "typescript",
    "tailwindcss",
    "vite",
    "hrnet"
  ],
  "author": {
    "name": "Mamady KONTE"
  },
  "bugs": {
    "url": "https://github.com/openclassrooms-students/hrnet-plugin/issues"
  },
  "homepage": "https://github.com/openclassrooms-students/hrnet-plugin#readme",
  "dependencies": {
    "@types/node": "^20.11.24",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "usehooks-ts": "^2.15.1",
    "vite-plugin-dts": "^3.7.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.62",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.5"
  },
  "peerDependencies": {
    "clsx": "^2.1.0",
    "date-fns": "^2.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwindcss": "^3.4.1",
    "usehooks-ts": "^2.15.1"
  }
}
